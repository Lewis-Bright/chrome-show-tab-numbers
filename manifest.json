{
  "manifest_version": 3,
  "name": "Chrome Show Tab Numbers",
  "version": "1.1.4",
  "description": "A Chromium extension to show tab numbers.",
  "icons": {
    "128": "assets/icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["scripting", "tabGroups", "tabs"],
  "host_permissions": ["<all_urls>"]
}
